{
  "name": "@scaleleap/jest-polly",
  "description": "Integrate Jest with PollyJS for a smooth experience of HTTP recording.",
  "license": "MIT",
  "author": {
    "name": "Roman Filippov",
    "email": "roman@scaleleap.com",
    "url": "https://www.scaleleap.com/"
  },
  "homepage": "https://github.com/ScaleLeap/jest-polly#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:ScaleLeap/jest-polly.git"
  },
  "version": "1.5.0",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf lib",
    "lint": "npx eslint --ext .js,.ts src/",
    "prepare": "npm run snyk-protect",
    "semantic-release": "npx @scaleleap/semantic-release-config",
    "snyk-protect": "snyk protect",
    "test": "jest"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@pollyjs/adapter-node-http": "4.3.0",
    "@pollyjs/core": "4.3.0",
    "@pollyjs/persister-fs": "4.3.0",
    "@types/pollyjs__adapter-node-http": "2.0.0",
    "@types/pollyjs__core": "4.3.0",
    "@types/pollyjs__persister-fs": "2.0.0",
    "@types/setup-polly-jest": "0.5.0",
    "ci-info": "2.0.0",
    "lodash.merge": "4.6.2",
    "setup-polly-jest": "0.9.0",
    "snyk": "^1.316.1",
    "type-fest": "0.15.1"
  },
  "peerDependencies": {
    "@scaleleap/config": "1.6.1"
  },
  "devDependencies": {
    "@scaleleap/config": "2.0.1",
    "@scaleleap/utils": "1.7.2",
    "@types/ci-info": "2.0.0",
    "@types/jest": "25.2.1",
    "@types/lodash.merge": "4.6.6",
    "@types/node": "13.13.4",
    "@types/node-fetch": "2.5.7",
    "jest": "25.5.3",
    "node-fetch": "2.6.0",
    "rimraf": "3.0.2",
    "ts-jest": "25.4.0",
    "tsconfigs": "4.0.2",
    "typescript": "3.9.2"
  },
  "keywords": [
    "fetch",
    "http",
    "jest",
    "polly",
    "pollyjs",
    "record",
    "replay",
    "snapshot"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "@scaleleap/semantic-release-config"
  },
  "snyk": true
}

{
  "name": "@scaleleap/jest-polly",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Integrate Jest with PollyJS for a smooth experience of HTTP recording.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "xo",
    "test": "jest",
    "build": "tsc"
  },
  "keywords": [
    "jest",
    "polly",
    "pollys",
    "http",
    "fetch",
    "record",
    "replay",
    "snapshot"
  ],
  "author": {
    "name": "Roman Filippov",
    "email": "roman@scaleleap.com",
    "url": "https://www.scaleleap.com/"
  },
  "homepage": "https://github.com/ScaleLeap/amazon-marketplaces#readme",
  "license": "MIT",
  "devDependencies": {
    "@jedmao/semantic-release-npm-github-config": "1.0.9",
    "@types/ci-info": "2.0.0",
    "@types/jest": "25.1.1",
    "@types/lodash.merge": "4.6.6",
    "@types/node": "13.7.0",
    "@types/node-fetch": "2.5.4",
    "@types/pollyjs__adapter-node-http": "2.0.0",
    "@types/pollyjs__core": "4.0.0",
    "@types/pollyjs__persister-fs": "2.0.0",
    "@types/setup-polly-jest": "0.5.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "eslint-config-xo-typescript": "0.26.0",
    "jest": "25.1.0",
    "node-fetch": "2.6.0",
    "ts-jest": "25.2.0",
    "tsconfigs": "4.0.2",
    "typescript": "3.7.5",
    "xo": "0.25.3"
  },
  "dependencies": {
    "@pollyjs/adapter-node-http": "4.0.3",
    "@pollyjs/core": "4.0.2",
    "@pollyjs/persister-fs": "4.0.2",
    "ci-info": "2.0.0",
    "lodash.merge": "4.6.2",
    "setup-polly-jest": "0.6.0"
  },
  "xo": {
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ],
    "envs": [
      "node",
      "jest"
    ],
    "space": true,
    "semicolon": false,
    "prettier": false,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "release": {
    "extends": "@jedmao/semantic-release-npm-github-config",
    "branch": "master"
  }
}
